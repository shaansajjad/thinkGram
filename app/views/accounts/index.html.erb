<div class="container">
	<div class="row">
		<div class="col-8">
			<% @posts.each do |post|%>
				<div class="post">
					<div class="post-meta">	
						<%= post.account.username%>
					 </div>
						<%= image_tag post.image.url, class: "img img-fluid"%>
				</div>
			<%end%>
		</div>
		<div class="col-4">
			<div class="account-summary">
				
			</div>
		</div>
	</div>
</div>